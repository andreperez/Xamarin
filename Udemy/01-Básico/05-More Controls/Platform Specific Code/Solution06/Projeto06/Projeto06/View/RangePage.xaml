<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Projeto06.View.RangePage">
  <StackLayout>
    <Label x:Name="txtSlider"></Label>
    <Slider Minimum="0" Maximum="1000" Value="300" ValueChanged="OnSliderValueChanged"></Slider>

    <Label x:Name="txtStepper"></Label>
    <Stepper Minimum="0" Maximum="10" Increment="2" Value="0" ValueChanged="OnStepperValueChanged"></Stepper>
    
  </StackLayout>
</ContentPage>